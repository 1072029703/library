<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="spring.common.test" class="org.beangle.commons.context.testbean.TestModule"/>

  <bean id="baseTransactionProxy" class="org.beangle.commons.context.testbean.ProxyFactoryBean"
    abstract="true">
  </bean>

  <bean id="entityDao" parent="baseTransactionProxy">
    <property name="target">
      <bean
        class="org.beangle.commons.context.testbean.TestEntityDao"
        autowire="byName">
      </bean>
    </property>
  </bean>
</beans>
