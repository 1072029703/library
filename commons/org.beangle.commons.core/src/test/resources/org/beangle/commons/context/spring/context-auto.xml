<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="autoConfigProcessor" class="org.beangle.commons.context.spring.SpringConfigProcessor" >
    <property name="reconfigResources">
      <bean class="org.beangle.commons.context.spring.SpringResources">
        <property name="locations" value="classpath*:spring-config.xml" />
      </bean>
    </property>
  </bean>

  <bean id="spring.common.test" class="org.beangle.commons.context.testbean.TestModule"/>
  <bean id="baseTransactionProxy" class="org.beangle.commons.context.testbean.ProxyFactoryBean"
    abstract="true">
  </bean>


  <bean id="entityDao" parent="baseTransactionProxy">
    <property name="target">
      <bean
        class="org.beangle.commons.context.testbean.TestEntityDao"
        autowire="byName">
      </bean>
    </property>
  </bean>
</beans>
