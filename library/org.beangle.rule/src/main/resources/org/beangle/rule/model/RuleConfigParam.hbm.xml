<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- 业务规则表参数 -->
<hibernate-mapping package="org.beangle.rule.model" >
	<class name="RuleConfigParam">
		<id name="id" unsaved-value="null">
			<generator class="org.beangle.model.persist.hibernate.support.TableSeqGenerator"/>
		</id>
		<many-to-one name="ruleConfig" />
		<many-to-one name="param"/>
		<property name="value"/>
	</class>
</hibernate-mapping>
