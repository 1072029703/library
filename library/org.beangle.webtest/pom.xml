<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.or g/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.beangle</groupId>
	<artifactId>beangle-webtest</artifactId>
	<version>1.0.0</version>
	<packaging>
		jar<!--bundle-->
	</packaging>
	<name>Beangle Web Test Toolkit</name>

	<properties>
		<downloadSources>true</downloadSources>
        <downloadJavadocs>true</downloadJavadocs>
	
		<bundle.symbolicName>org.beangle.webtest</bundle.symbolicName>
		<bundle.namespace>org.beangle.webtest</bundle.namespace>

		<testng.version>6.3.1</testng.version>
		<selenium-java.version>2.19.0</selenium-java.version>
		<reportng.version>1.1.2</reportng.version>
		<guice.version>2.0</guice.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>org.seleniumhq.selenium</groupId>
			<artifactId>selenium-java</artifactId>
			<version>${selenium-java.version}</version>
		</dependency>
		<dependency>
			<groupId>org.testng</groupId>
			<artifactId>testng</artifactId>
			<version>${testng.version}</version>
		</dependency>
		<dependency>
			<groupId>com.google.inject</groupId>
			<artifactId>guice</artifactId>
			<version>${guice.version}</version>
		</dependency>
<!--  
		<dependency>
			<groupId>org.uncommons</groupId>
			<artifactId>reportng</artifactId>
			<version>${reportng.version}</version>
			<exclusions>
				<exclusion>
					<groupId>org.testng</groupId>
					<artifactId>testng</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
-->
	</dependencies>
</project>
