<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<mapping class="org.beangle.security.blueprint.model.RoleBean"/>
		<mapping class="org.beangle.security.blueprint.model.MemberBean"/>
		<mapping class="org.beangle.security.blueprint.model.UserBean"/>

		<mapping class="org.beangle.security.blueprint.function.model.FuncPermissionBean"/>
		<mapping class="org.beangle.security.blueprint.function.model.FuncResourceBean"/>

		<mapping class="org.beangle.security.blueprint.data.model.DataPermissionBean"/>
		<mapping class="org.beangle.security.blueprint.data.model.DataResourceBean"/>
		<mapping class="org.beangle.security.blueprint.data.model.DataFieldBean"/>
		
		<mapping class="org.beangle.security.blueprint.data.model.RoleProfileBean"/>
		<mapping class="org.beangle.security.blueprint.data.model.RolePropertyBean"/>
		<mapping class="org.beangle.security.blueprint.data.model.UserProfileBean"/>
		<mapping class="org.beangle.security.blueprint.data.model.UserPropertyBean"/>
		
		<mapping class="org.beangle.security.blueprint.nav.model.MenuBean"/>
		<mapping class="org.beangle.security.blueprint.nav.model.MenuProfileBean"/>
		
		<mapping class="org.beangle.security.blueprint.session.model.SessionProfileBean"/>
	</session-factory>
</hibernate-configuration>