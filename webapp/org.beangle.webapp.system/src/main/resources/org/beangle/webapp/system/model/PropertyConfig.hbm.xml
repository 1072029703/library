<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
		
<hibernate-mapping package="org.beangle.webapp.system.model">
	<class name="PropertyConfig">
		<id name="id" unsaved-value="null">
			<generator class="org.beangle.model.persist.hibernate.support.TableSeqGenerator"/>
		</id>
		<property name="name" not-null="true" length="50"/>
		<property name="value"  not-null="true" length="200"/>
		<property name="description" not-null="true" length="200"/>
		<property name="type" not-null="true" length="200"/>
	</class>
</hibernate-mapping>