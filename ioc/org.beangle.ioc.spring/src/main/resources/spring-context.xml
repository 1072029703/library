<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="BeangeSpringConfigProcessor" class="org.beangle.commons.context.spring.SpringConfigProcessor">
    <property name="reconfigResources">
      <bean class="org.beangle.commons.context.spring.SpringResources">
        <property name="locations" value="classpath*:spring-config.xml" />
      </bean>
    </property>
  </bean>

  <import resource="classpath*:META-INF/spring/*.xml" />
  <import resource="classpath*:META-INF/spring-override/*.xml" />
  <import resource="classpath*:spring-local.xml" />
</beans>
